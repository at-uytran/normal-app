<div class="col-md-8">
  <script src="https://cdn.jwplayer.com/libraries/mX8w3yYs.js"></script>
  <div id="myPlayer">This text will be replaced with a player.</div>
  <script>
    jwplayer("myPlayer").setup({
      file: "https://uytv-hls.s3.amazonaws.com/demo.m3u8",
      height: "90%",
      width: "100%"
    });
  </script>
</div>
<div class="col-md-4">
  <section class="content">
    <div class="card p-2">
      <div class="card-header">
        <h3 class="card-title">Recently Added Videos</h3>

        <div class="card-tools">
          <button type="button" class="btn btn-tool" data-card-widget="collapse">
            <i class="fas fa-minus"></i>
          </button>
          <button type="button" class="btn btn-tool" data-card-widget="remove">
            <i class="fas fa-times"></i>
          </button>
        </div>
      </div>
      <div class="card-body p-2">
        <ul class="products-list product-list-in-card pl-2 pr-2">
          <li class="item">
            <div class="product-img">
              <img src="dist/img/default-150x150.png" alt="<%= "Video thumb" %>" class="img-size-50">
            </div>
            <div class="product-info">
              <a href="javascript:void(0)" class="product-title"><%= "Video name" %>
                <span class="badge badge-warning float-right">$1800</span></a>
              <span class="product-description">
                <%= "Video description" %>
              </span>
            </div>
          </li>
        </ul>
      </div>
    </div>
  </section>
</div>
